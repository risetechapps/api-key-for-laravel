{
    "name": "risetechapps/api-key-for-laravel",
    "description": "",
    "keywords": [
        "risetechapps",
        "service",
        "auth",
        "api",
        "key"
    ],
    "homepage": "https://github.com/risetechapps/api-key-for-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Rise Tech",
            "email": "apps@risetech.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12",
        "laravel/sanctum": "^4.0",
        "risetechapps/address-for-laravel": "^1.0.0",
        "risetechapps/code-generate": "^1.0",
        "risetechapps/form-request-for-laravel": "^1.0.0",
        "risetechapps/media-for-laravel": "^1.0.0",
        "risetechapps/to-upper-for-laravel": "^1.0",
        "risetechapps/has-uuid-for-laravel": "^1.0",
        "tpetry/laravel-postgresql-enhanced": "^3.5.0",
        "laravel/tinker": "^2.10.1",
        "risetechapps/risetools": "^1.2.0",
        "risetechapps/repository-for-laravel" : "^1.1.0",
        "risetechapps/view-suite": "^1.0.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "RiseTechApps\\ApiKey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RiseTechApps\\ApiKey\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiseTechApps\\ApiKey\\ApiKeyServiceProvider"
            ],
            "aliases": {
                "ApiKey":  "RiseTechApps\\ApiKey\\ApiKeyFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.0.0"

}
